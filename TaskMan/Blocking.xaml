<Window x:Class="TaskMan.Blocking"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaskMan"
        mc:Ignorable="d"
        Title="Блокировка/Разблокировка" Height="600" Width="300" ResizeMode="NoResize" >
    <StackPanel>
        <WrapPanel Margin="3,5,-1,5">
            <Button Name="btnBlock" Height="20" Width="140" Content="Блокировка" Click="btnBlock_Click" HorizontalAlignment="Left"></Button>
            <Button Name="btnUnBlock" Height="20" Width="140" Content="Разблокировка" Click="btnUnBlock_Click" HorizontalAlignment="Right"></Button>
        </WrapPanel>
        <!--<CheckBox Name="chkHDD" Content="Спрятать HDD" Margin="5,0,0,0"/>
        <CheckBox Name="chkTask" Content="Подмена диспетчера задач" Margin="5,0,0,0"/>
        <CheckBox Name="chkProgramHide" Content="Замена пункта программы" Margin="5,0,0,0"/>-->
        <ListBox Name="lstRegBlock" Width="280" Height="520" Margin="0,5,0,5"/>
    </StackPanel>
</Window>
